{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
	    {
			"path": "pages/noLayouts/login/index",
			"style": {
				"navigationBarTitleText": "登录",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/tabBar/home/index",
			"style": {
				"navigationBarTitleText": "首页",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/tabBar/stock/index",
			"style": {
				"navigationBarTitleText": "库存",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/tabBar/order/list/index", // 订单-列表
			"style": {
				"navigationBarTitleText": "订单",
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/tabBar/order/add/index", // 订单-新增
			"style": {
				"navigationBarTitleText": "新增订单",
				"enablePullDownRefresh": true
			}

		},
		{
			"path": "pages/tabBar/mine/index",
			"style": {
				"navigationBarTitleText": "我的",
				"enablePullDownRefresh": false
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#fff",
		"backgroundColor": "#fff"
	},
	"tabBar": {
		"list": [
			{ "pagePath": "pages/tabBar/home/index" },
			{ "pagePath": "pages/tabBar/stock/index" },
			{ "pagePath": "pages/tabBar/order/list/index" },
			{ "pagePath": "pages/tabBar/mine/index" }
		]
	},
	"uniIdRouter": {}
}